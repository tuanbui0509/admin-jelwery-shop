<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myadmin">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:usesCleartextTraffic="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyAdmin">
        <activity android:name=".pageLogin">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".EditProduct" />
        <activity android:name=".PieChartActivity" />
        <activity android:name=".AddProduct" />
        <activity android:name=".ListUserActivity" />
        <activity android:name=".listProductInvoice" />
        <activity android:name=".ProductActivity" />
        <activity android:name=".InvoiceActivity" />
        <activity android:name=".MainActivity">
        </activity>
    </application>

</manifest>